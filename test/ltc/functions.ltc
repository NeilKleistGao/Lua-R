function max(num1, num2)
  if (num1 > num2) then
    result = num1;
  else
    result = num2;
  end
  return result;
end

function maximum (a)
  local mi = 1
  local m = a[mi]
  for i,val in ipairs(a) do
    if val > m then
      mi = i
       m = val
    end
  end
  return m, mi
end

function add(...)
  local s = 0
  for i, v in ipairs{...} do
    s = s + v
  end
  return s
end
